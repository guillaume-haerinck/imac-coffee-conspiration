{
  "name": "imac-frontend-coffe",
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "sass": "sass --watch --no-source-map src/styles.scss:src/_styles.css",
    "typescript": "tsc --watch",
    "browserify": "watchify src/main.js -o src/_bundle.js -v",
    "live": "browser-sync start --config bs-config.js",
    "serve": "npm-run-all --parallel typescript sass browserify live",
    "sass:build": "sass --no-source-map src/styles.scss:src/_styles.css --style compressed",
    "typescript:build": "tsc",
    "browserify:build": "browserify src/main.js -o src/_bundle.js",
    "uglify": "uglifyjs src/_bundle.js -o src/_bundle.js",
    "build": "npm-run-all sass:build typescript:build browserify:build uglify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guillaume-haerinck/imac-frontend-coffe.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guillaume-haerinck/imac-frontend-coffe/issues"
  },
  "homepage": "https://github.com/guillaume-haerinck/imac-frontend-coffe#readme",
  "dependencies": {
    "matter-js": "^0.14.2",
    "@types/matter-js": "^0.10.2"
  },
  "devDependencies": {
    "browserify": "^16.2.3",
    "uglify-js": "^3.4.9",
    "watchify": "^3.11.0"
  }
}
